package com.ku.dku.bean;

import java.util.Date;

public class SetDateBookRoomRequest {

	private java.sql.Date reserveStart;
	private java.sql.Date reserveDue;
	private long termId;
	private Integer year;
	
	
	public SetDateBookRoomRequest() {
		super();
	}
	public SetDateBookRoomRequest(java.sql.Date reserveStart, java.sql.Date reserveDue, long termId, Integer year) {
		super();
		this.reserveStart = reserveStart;
		this.reserveDue = reserveDue;
		this.termId = termId;
		this.year = year;
	}
	public java.sql.Date getReserveStart() {
		return reserveStart;
	}
	public void setReserveStart(java.sql.Date reserveStart) {
		this.reserveStart = reserveStart;
	}
	public java.sql.Date getReserveDue() {
		return reserveDue;
	}
	public void setReserveDue(java.sql.Date reserveDue) {
		this.reserveDue = reserveDue;
	}
	public long getTermId() {
		return termId;
	}
	public void setTermId(long termId) {
		this.termId = termId;
	}
	public Integer getYear() {
		return year;
	}
	public void setYear(Integer year) {
		this.year = year;
	}
	
	
	
}
