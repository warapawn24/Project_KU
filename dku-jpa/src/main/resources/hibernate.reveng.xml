<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-reverse-engineering SYSTEM "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd">
<hibernate-reverse-engineering>
 <schema-selection match-catalog="dku" match-schema="public" />
 <type-mapping>
  <sql-type jdbc-type="CHAR" hibernate-type="string" />
  <sql-type jdbc-type="VARCHAR" hibernate-type="string" />
  <sql-type jdbc-type="NUMERIC" hibernate-type="java.math.BigDecimal" />
  <sql-type jdbc-type="INTEGER" hibernate-type="java.lang.Long" />
  <sql-type jdbc-type="BIGINT" hibernate-type="java.lang.Long" />
  <sql-type jdbc-type="DECIMAL" hibernate-type="java.math.BigDecimal" />
 </type-mapping>
 
 <table-filter match-name="DATABASECHANGELOG" exclude="true" />
 <table-filter match-name="DATABASECHANGELOGLOCK" exclude="true" />
 <table-filter match-name="databasechangelog" exclude="true" />
 <table-filter match-name="databasechangeloglock" exclude="true" />
